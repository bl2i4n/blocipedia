<h1>Wikis#show</h1>
<p>Find me in app/views/wikis/show.html.erb</p>

<h1><%= @wiki.title %></h1>

<div class="row">
  <div class="col-md-8">
    <%= markdown(@wiki.body) %>
  </div>
  <div class="col-md-4">
<!-- #5 -->
    <%= link_to "Edit", edit_wiki_path(@wiki), class: 'btn btn-success' %>
    <%= link_to "Delete Article", @wiki, method: :delete, class: 'btn btn-danger', data: {confirm: 'Are you sure you want to delete this post?'} %>
  </div>
</div>
